  color: #B6B6B6;
  margin: 2px 0 0;
}
.carlistbox .cartealist .oneteainfo .cteaprice{
  display: block;
  font-size: 14px;
  margin: 10px 0 0;
}
.carlistbox .countbox{
  display: flex;
  align-items: center;
}
.carlistbox .reduce{
  width: 15px;
  height: 15px;
  display: flex;
  margin: 0 15px 0 0;
  align-items: center;
}
.carlistbox .reducein{
  width: 15px;
  height: 3px;
  border-radius: 1px;
  background: #666;
  
}
.carlistbox .teanumber{

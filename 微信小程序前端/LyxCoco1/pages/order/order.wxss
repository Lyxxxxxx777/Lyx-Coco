  box-sizing: border-box;
  display: flex;
  justify-content: space-between;
  align-items: center;
  border-bottom: 1px solid #fafafa;
}

.carlistbox .cartealist .oneteainfo .cteaname{
  display: block;
  font-size: 15px;
}
.carlistbox .cartealist .oneteainfo .cteafeed{
  display: block;

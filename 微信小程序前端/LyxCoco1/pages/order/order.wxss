  font-size: 16px;
}
.carlistbox .myselected .cltop{
  z-index: 28;
  border-top-right-radius: 15px;
  border-top-left-radius: 15px;
  height: 30px;
  background: #FFEDDC;
  color: #FF8305;
  text-align: center;
  font-size: 12px;
  font-weight: 400;
  line-height: 30px;
}
.carlistbox .yxcp{
  margin: 20px 0 0 20px;
}
.carlistbox .clearbtn{
  /* background: pink url('https://z3.ax1x.com/2021/03/27/6zZTBD.png') no-repeat center center; */
  position: absolute;
  top: 50px;
  right: 20px;
  height: 20px;
  display: flex;
  /* background: pink; */
}
.carlistbox .clearbtn image{
  width: 20px;
  margin-right: 5px;
}
.carlistbox .clearbtn span{
  height: 20px;
  line-height: 20px;
